{
  "name": "familyhub",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "docker:up": "docker-compose up -d",
    "docker:up:db": "docker-compose up -d db",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "cap:sync": "nuxt build && cap sync",
    "cap:sync:ios": "nuxt build && cap sync ios",
    "cap:sync:android": "nuxt build && cap sync android",
    "cap:open:ios": "cap open ios",
    "cap:open:android": "cap open android",
    "cap:run:ios": "cap run ios",
    "cap:run:android": "cap run android",
    "ios": "nuxt build && cap sync ios && cap open ios",
    "android": "nuxt build && cap sync android && cap open android"
  },
  "dependencies": {
    "@capacitor/barcode-scanner": "^3.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@capgo/capacitor-nfc": "^8.0.5",
    "@nuxt/ui": "^4.2.1",
    "bcrypt": "^6.0.0",
    "drizzle-orm": "^0.45.1",
    "nuxt": "^4.2.2",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/lucide": "^1.2.80",
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/eslint-config": "^1.12.1",
    "@types/bcrypt": "^6.0.0",
    "@types/qrcode": "^1.5.6",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  }
}
